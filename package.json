{
  "name": "homeportal-api",
  "private": true,
  "version": "1.0.0",
  "description": "GraphQL server for homeportal",
  "author": "Darius Bakunas-Milanowski <bakunas@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "nodemon api/index.js --exec babel-node --inspect=5858",
    "build": "babel api -d dist",
    "serve": "node dist/index.js"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "dataloader": "^1.4.0",
    "dotenv": "^5.0.1",
    "express": "^4.14.1",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "morgan": "^1.8.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/cli": " v7.0.0-beta.42"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "chrome": 52,
            "browsers": [
              "last 2 versions",
              "safari 7"
            ]
          }
        }
      ]
    ]
  }
}
